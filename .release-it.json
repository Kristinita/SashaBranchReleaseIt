{
    "buildCommand": "changelog -u https://${repo.host}/${repo.repository} -f Changelog.md && sed -i 's/^version = .*$/version = ${version}/g' setup.cfg",
    "changelogCommand": "changelog -f -",
    "github": {
        "release": true,
        "tokenRef": "GITHUB_TOKEN"
    },
    "safeBump": false
}
